{
  "expo": {
    "name": "fanon-mobile",
    "slug": "fanon-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fanon-icon.png",
    "scheme": "fanonmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/fanon_splash.jpg",
      "resizeMode": "cover",
      "backgroundColor": "#faff00"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/fanon-icon.png",
        "backgroundColor": "#faff00"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.immortalcodes.fanonmobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/images/fanon-icon.png",
          "color": "#3B82F6",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "b599f28d-7274-46c7-b3ca-eecc9d5d063f"
      }
    },
    "owner": "immortalcodes"
  }
}
