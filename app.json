{
  "expo": {
    "name": "fanon-mobile",
    "slug": "fanon-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/fanon-icon.png",
    "scheme": "fanonmobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/fanon_splash.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#121212"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundImage": "./assets/images/fanon-icon.png",
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.immortalcodes.fanonmobile"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/fanon_splash.jpg",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#121212",
          "dark": {
            "backgroundColor": "#121212"
          }
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/fanon-icon.png",
          "color": "#3B82F6",
          "defaultChannel": "default"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "f64419e2-89aa-489c-95e8-000b8305f07f"
      }
    },
    "owner": "immortalcodes"
  }
}
